<?xml version="1.0" encoding="UTF-8"?>
<module>
	<name>Editor-Template  Manager</name>
	<version>1.0.0</version>
	<requirements>
		<minimum_version>3.6.0</minimum_version>
	</requirements>
	<description>
		<![CDATA[Modul zum erstellen und editieren von Editor-Templates]]>
	</description>
	<authors>
		<author>
			<name><![CDATA[Kemper &amp; Schlomski GmbH]]></name>
			<url><![CDATA[http://kemper-schlomski.de]]></url>
		</author>
	</authors>
	<events>
		<event application="backend" name="after_add"><![CDATA[Triggered when a Editor-Template  Manager is added.]]></event>
		<event application="backend" name="after_delete"><![CDATA[Triggered when a Editor-Template  Manager is deleted.]]></event>
		<event application="backend" name="after_edit"><![CDATA[Triggered when a Editor-Template  Manager is edited.]]></event>
		<event application="backend" name="after_saved_settings"><![CDATA[Triggered when settings are saved.]]></event>
	</events>
</module>